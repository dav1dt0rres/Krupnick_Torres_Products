    function addCell(tr, val,response,right_answer) {
        //console.log("response and right answer"+" "+response+" "+right_answer+" "+val);
        if (response!=right_answer){
            // console.log("incorrect")
            var td = document.createElement('td');
            td.style.color='red'
            td.innerHTML = val;

        }
        else{
            //console.log("correct")
            var td = document.createElement('td');
            td.style.color='black'
            td.innerHTML = val;
            //td.value=val;
        }

        tr.appendChild(td)
    }
    function addRow(tbl, val_1, val_2, val_3,val_4){
        var tr = document.createElement('tr');

        addCell(tr, val_1);
        addCell(tr, val_2);
        addCell(tr, val_3);
        addCell(tr, val_4);

        tr.onclick=async function(event) {

            document.getElementById("Final_Questions_holder").value=val_1
            console.log('clicked!!!! '+document.getElementById("form").submit());

        }
        tbl.appendChild(tr)
    }

